{
  "name": "swgoh",
  "version": "0.3.2",
  "description": "Unofficial node library for swgoh.gg",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/pikax/swgoh.git",
  "author": "pikax",
  "license": "MIT",
  "scripts": {
    "dist": "rollup -c",
    "test": "jest",
    "snapshot": "jest --updateSnapshot"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/__tests__/__config__/preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/*.(ts|tsx|js)"
    ]
  },
  "dependencies": {
    "@types/cheerio": "^0.22.2",
    "@types/request": "^2.0.3",
    "cheerio": "^1.0.0-rc.2",
    "promise-queue": "^2.2.3",
    "request": "^2.81.0",
    "requestretry": "^1.12.2"
  },
  "devDependencies": {
    "@types/jest": "^21.1.8",
    "jest": "^21.2.1",
    "rollup": "^0.49.2",
    "rollup-plugin-typescript2": "^0.5.2",
    "typescript": "^2.5.2"
  }
}
